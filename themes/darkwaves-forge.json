        "entity.name.class",
        "entity.name.class.js",
        "entity.name.class.ts",
        "entity.name.class.tsx",
        "entity.name.class.jsx"
      ],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Function",
