    {
      "name": "HTML Comments",
      "scope": ["comment.block.html", "comment.block.xml"],
      "settings": {
        "foreground": "#6A6F76",
        "fontStyle": "italic"
      }
    },
    {
      "name": "HTML Meta Tags",
      "scope": ["meta.tag.html", "meta.tag.xml"],
      "settings": {
        "foreground": "#D5B06B"
      }
    },
    {
      "name": "HTML Script Tags",
      "scope": ["entity.name.tag.script.html", "entity.name.tag.style.html"],
