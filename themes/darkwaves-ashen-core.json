  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6A6F76"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.quoted.double",
        "string.quoted.single",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#8FCB6D"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex",
        "constant.numeric.binary",
        "constant.numeric.octal"
      ],
      "settings": {
        "foreground": "#D87D7D"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.control.throw",
        "keyword.control.try",
        "keyword.control.catch",
        "keyword.control.finally",
        "keyword.control.switch",
        "keyword.control.case",
        "keyword.control.default",
        "keyword.control.break",
        "keyword.control.continue"
      ],
      "settings": {
        "foreground": "#E67E62"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#5C90EF"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable.other.readwrite",
        "variable.other.property",
        "variable.parameter",
        "variable.other",
        "support.variable",
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#C6C6C6"
      }
    },
    {
      "name": "Type",
      "scope": [
        "entity.name.type",
        "support.type",
        "support.type.primitive",
        "entity.other.inherited-class",
        "entity.name.interface",
        "entity.name.type.alias"
      ],
      "settings": {
        "foreground": "#D5B06B"
      }
    },
    {
      "name": "Class Names",
      "scope": [
        "entity.name.class",
        "entity.name.interface",
        "entity.name.enum",
        "entity.name.struct"
      ],
      "settings": {
        "foreground": "#D5B06B"
      }
    },
    {
      "name": "CSS Units",
      "scope": [
        "keyword.other.unit.css",
        "keyword.other.unit.scss",
        "keyword.other.unit.less"
      ],
      "settings": {
        "foreground": "#E67E62"
      }
    },
    {
      "name": "CSS Numeric Values",
      "scope": [
        "constant.numeric.css",
        "constant.numeric.scss",
        "constant.numeric.less"
      ],
      "settings": {
        "foreground": "#E67E62"
      }
    },
    {
      "name": "PHP Variables",
      "scope": [
        "variable.other.php",
        "variable.language.php",
        "punctuation.definition.variable.php"
      ],
      "settings": {
        "foreground": "#D6D6D6"
      }
    },
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag.html",
        "entity.name.tag.jsx",
        "entity.name.tag.tsx",
        "entity.name.tag.xml"
