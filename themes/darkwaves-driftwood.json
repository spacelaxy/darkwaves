  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6A6660"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.control.throw",
        "keyword.control.try",
        "keyword.control.catch",
        "keyword.control.finally",
        "keyword.control.switch",
        "keyword.control.case",
        "keyword.control.default",
        "keyword.control.break",
        "keyword.control.continue"
      ],
      "settings": {
        "foreground": "#C49A6C"
      }
    },
    {
      "name": "Class Keywords",
      "scope": [
        "keyword.control.class",
        "keyword.control.extends",
        "keyword.control.implements",
        "storage.modifier.class",
        "storage.modifier.extends",
        "storage.modifier.implements"
      ],
      "settings": {
        "foreground": "#C49A6C"
      }
    },
    {
      "name": "Return Italic",
      "scope": [
        "keyword.control.flow",
        "keyword.control.return",
        "keyword.control"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call.generic"
